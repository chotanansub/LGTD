{
  "dataset": {
    "name": "synth2",
    "path": "data/synthetic/datasets/synth2_data.json",
    "trend_type": "inverted_v",
    "period_type": "fixed",
    "period": 60
  },
  "models": {
    "lgtd": {
      "enabled": true,
      "params": {
        "window_size": 3,
        "error_percentile": 40,
        "trend_selection": "auto",
        "lowess_frac": 0.1,
        "threshold_r2": 0.92,
        "verbose": false
      }
    },
    "lgtd_linear": {
      "enabled": true,
      "params": {
        "window_size": 3,
        "error_percentile": 40,
        "lowess_frac": 0.1,
        "verbose": false
      }
    },
    "lgtd_lowess": {
      "enabled": true,
      "params": {
        "window_size": 3,
        "error_percentile": 40,
        "lowess_frac": 0.1,
        "verbose": false
      }
    },
    "stl": {
      "enabled": true,
      "params": {
        "period": 60,
        "seasonal": 13,
        "trend": null,
        "robust": false
      }
    },
    "robust_stl": {
      "enabled": false,
      "params": {
        "period": 12,
        "reg1": 10.0,
        "reg2": 0.5,
        "K": 2,
        "H": 5,
        "dn1": 1.0,
        "dn2": 1.0,
        "ds1": 50.0,
        "ds2": 1.0
      }
    },
    "fast_robust_stl": {
      "enabled": true,
      "params": {
        "period": 120,
        "reg1": 1.0,
        "reg2": 10.0,
        "K": 2,
        "H": 5,
        "dn1": 1.0,
        "dn2": 1.0,
        "ds1": 50.0,
        "ds2": 1.0,
        "max_iter": 1000
      }
    },
    "str": {
      "enabled": true,
      "params": {
        "seasonal_periods": [
          120
        ],
        "trend_lambda": 100.0,
        "seasonal_lambda": 1.0,
        "robust": false,
        "auto_params": false,
        "n_trials": 20
      }
    },
    "online_stl": {
      "enabled": true,
      "params": {
        "periods": [
          60
        ],
        "lam": 0.1,
        "init_window_ratio": 0.3
      }
    },
    "oneshot_stl": {
      "enabled": true,
      "params": {
        "period": 60,
        "shift_window": 0,
        "init_ratio": 0.3
      }
    },
    "astd": {
      "enabled": true,
      "params": {
        "seasonality_smoothing": 0.7,
        "mode": "batch"
      }
    },
    "astd_online": {
      "enabled": true,
      "params": {
        "seasonality_smoothing": 0.7,
        "mode": "online",
        "init_window_size": 300
      }
    }
  },
  "evaluation": {
    "metrics": [
      "mse",
      "mae",
      "rmse",
      "correlation",
      "psnr"
    ],
    "save_plots": true,
    "plot_dir": "experiments/results/synthetic/figures/synth2"
  }
}